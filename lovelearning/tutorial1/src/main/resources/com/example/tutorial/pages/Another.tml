<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<t:pagelink page="Start">pagelink:Start</t:pagelink><br/>
	<p xml:space="preserve">
		<a href="#" t:type="pagelink" page="Start">html Start</a><br/>
		<a href="#" t:id="goToStart">class Start</a>
	</p>
	The value is: ${prop:value}
	
	<script type="text/javascript">
	
		var amq = org.activemq.Amq;
		  amq.init({ 
		    uri: 'amq', 
		    logging: true,
		    timeout: 20,
		    clientId : (new Date()).getTime().toString()
		  });
	
		function listener(message) {
                jQuery("#receive").append("<p>" + message.nodeType + "." + message.nodeValue + "</p>");
        }; 
        
         function pollHandler(first) {
                if (first) {
                    amq.addListener("test", "topic://test", listener);
                }
         }; 
         
         
          jQuery(function(){
        	  
                jQuery("#send").click(function(){
                    amq.sendMessage("topic://test", "[\"Hello\", \"Hello\"]");
                });
                jQuery("#clear").click(function(){
                    jQuery("#receive").html("");
                });
                //amq.addPollHandler(pollHandler);
            }) 
</script>

	<div>
        <button id="send">send</button>
        <button id="clear">clear</button>
        <div id="receive"></div>
  	</div>
</html>